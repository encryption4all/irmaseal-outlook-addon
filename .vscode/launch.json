        {
            "name": "Office Online (Chrome)",
            "type": "chrome",
            "request": "launch",
            "webRoot": "${workspaceFolder}",
            "preLaunchTask": "Debug: Web"
        },
        {
            "name": "Office Online (Edge Chromium)",
            "type": "pwa-msedge",
            "request": "launch",
            "port": 9222,
            "webRoot": "${workspaceFolder}",
            "preLaunchTask": "Debug: Web"
        },
        {
            "type": "node-terminal",
            "name": "Run Script: dev-server",
            "request": "launch",
            "command": "npm run dev-server",
            "cwd": "${workspaceFolder}"
        }
    ]
}
