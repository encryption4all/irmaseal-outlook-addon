{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "edge",
            "request": "attach",
            "name": "Attach to Office Add-ins",
            "port": 9222,
            "trace": "verbose",
            "url": "https://localhost:3000/taskpane.html?_host_Info=Outlook$Win32$16.01$en-US$$$$0",
            "webRoot": "${workspaceFolder}",
            "timeout": 45000
        },
        {
            "name": "Debug Office Add-in (Edge Chromium)",
            "type": "edge",
            "request": "attach",
            "useWebView": "advanced",
            "port": 9229,
            "timeout": 60000,
            "webRoot": "${workspaceRoot}",
            "sourceMapPathOverrides": {
                "webpack:///./*": "${webRoot}/*" // Example: "webpack:///./src/app.js" -> "/Users/me/project/src/app.js",
            },
            "address": "127.0.0.1"
        },
        {
            "name": "Excel Desktop (Edge Chromium)",
            "type": "edge",
            "request": "attach",
            "useWebView": "advanced",
            "port": 9229,
            "timeout": 600000,
            "webRoot": "${workspaceRoot}",
            "preLaunchTask": "Debug: Excel Desktop",
            "postDebugTask": "Stop Debug"
        },
        {
            "name": "Excel Desktop (Edge Legacy)",
            "type": "office-addin",
            "request": "attach",
            "url": "https://localhost:3000/taskpane.html?_host_Info=Excel$Win32$16.01$en-US$$$$0",
            "port": 9222,
            "timeout": 600000,
            "webRoot": "${workspaceRoot}",
            "preLaunchTask": "Debug: Excel Desktop",
            "postDebugTask": "Stop Debug"
        },
        {
            "name": "Outlook Desktop (Edge Chromium)",
